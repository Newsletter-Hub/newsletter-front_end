{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "importOrder": [
    "next",
    "^next/(.*)$",
    "^@hookform/(.*)$",
    "clsx",
    "^@/pages/api/(.*)$",
    "^@/pages/(.*)$",
    "^@/config/(.*)$",
    "^@/hooks/(.*)$",
    "country-state-city",
    "date-fns",
    "^@/types/(.*)$",
    "^@react-oauth/(.*)$",
    "@radix-ui/(.*)$",
    "^@/components/(.*)$",
    "^@/assets/(.*)$",
    "^[./]",
    "@/styles/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
